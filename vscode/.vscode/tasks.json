{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "npm",
			"script": "webpack",
			"problemMatcher": "$tsc-watch",
			"isBackground": true,
			"presentation": {
				"reveal": "never"
			},
			"group": {
				"kind": "build",
				"isDefault": true
			}
    },
    {
      "label": "e2eCompile",
			"type": "npm",
			"script": "e2e:compile",
			"isBackground": false,
    },
    {
      "label": "e2e2Compile",
			"type": "npm",
			"script": "e2e2:compile",
			"isBackground": false,
    },
	]
}
